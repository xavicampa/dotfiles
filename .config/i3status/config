general {
        colors = true
        interval = 1
}

order += "volume master"
order += "ethernet _first_"
order += "tztime local"

ethernet _first_{
        format_up = "%ip"
        format_down = "down"
}

tztime local {
        format = "%Y-%m-%d %H:%M:%S"
}

volume master {
        format = "Vol: %volume"
        format_muted = ""
}
